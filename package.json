{
  "name": "bareblocks",
  "version": "0.0.0",
  "description": "Bare building blocks for a modern app with Gatsby, React, PostCSS and Typescript",
  "keywords": [
    "gatsby",
    "boilerplate",
    "typescript"
  ],
  "homepage": "https://github.com/alvis/bareblocks#readme",
  "bugs": {
    "url": "https://github.com/alvis/bareblocks/issues"
  },
  "license": "MIT",
  "author": {
    "name": "Alvis HT Tang",
    "email": "alvis@hilbert.space"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alvis/bareblocks.git"
  },
  "scripts": {
    "postinstall": "presetter bootstrap",
    "build": "run-s clean build:gatsby",
    "build:gatsby": "gatsby build --verbose",
    "clean": "run-p clean:*",
    "clean:babel": "shx rm -f .babelrc.json",
    "clean:cache": "shx rm -rf .cache",
    "clean:codegen": "shx rm -f types/@graphql.ts",
    "clean:public": "shx rm -rf public",
    "start": "run-s clean start:gatsby",
    "start:gatsby": "gatsby develop"
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "7.x",
    "@types/react": "16.x",
    "@types/react-helmet": "6.x",
    "@types/webpack-bundle-analyzer": "3.x",
    "babel-plugin-module-resolver": "4.x",
    "eslint-plugin-react": "7.x",
    "gatsby": "2.x",
    "gatsby-plugin-graphql-codegen": "2.x",
    "gatsby-plugin-react-helmet": "3.x",
    "gatsby-plugin-ts-config": "0.2.3",
    "gatsby-plugin-typescript": "2.x",
    "presetter": "1.x",
    "presetter-preset": "1.x",
    "webpack-bundle-analyzer": "3.x"
  },
  "dependencies": {
    "react-dom": "16.x",
    "react-helmet": "6.x"
  }
}
